.boosters-img.second{
	margin: 0 40px 50px 40px;
	background-position: 241px 0;

}

.boosters-img.third {
	margin: 0 40px 50px 40px;
	background-position: 499px 0;
}


.what.box {
	width: 910px;
	height: 42px;
}

.what.box.boosters {
	height:60px;
}

.gift-landing {
	margin:50px 0;
}

.gift-blurb span.blurb-text {
	font-size:16px;
	color:#595959;
	font-family:"proxima-nova", sans-serif;
	text-align: left;
}

.gift-blurb {
	float:left;
	margin: 0;
    text-align: left;
    width: 40%;

}

.super-convert {
	margin:40px 0 0 0;
}

.gift-image {
	margin: 0;
	float:right;
	width:60%;
}

.gift-image img {
	max-width:100%;
	margin-top:-50px;
}

.learn-more.chevron.down .chevron-triangle {
	border-color: white transparent transparent;
    border-style: solid;
    border-width: 50px 150px;
    height: 0;
    margin-left:-35px;
    margin-top: 0;
    position: absolute;
    width: 0;
    z-index: 1;
}

.learn-more.chevron.down .first-line.block {
	color: #595959;
    font-size: 10px;
    margin: 0 0 0 82px;
    position: absolute;
    text-align: center;
    z-index: 2;
}

.keep-calm-signmeup-container a.invisibut {
	margin: 0 auto;
    position: absolute;
}

a.convert, button.convert {
	font-family: ff-enzo-web,arial,sans-serif;
	text-align: center;
	box-shadow: 0pt 1px 0pt rgba(255, 255, 255, 0.5) inset, 0pt 1px 0pt rgba(255, 255, 255, 0.2);
	background-clip:padding-box;
	white-space: nowrap;
	text-decoration: none;
	font-weight: bold;
	margin:0;
	outline:0 none;
	cursor:default;
	border:none;
	text-shadow: 0pt -1px 0pt rgb(0, 0, 0);
	color:rgb(255, 255, 255);
	background-image:-moz-linear-gradient(center top , #66B84A, #51933A);
	background-image:-webkit-gradient(linear,left top,left bottom,color-stop(0, #66B84A),color-stop(1, #51933A));
	filter:progid:DXImageTransform.Microsoft.gradient(startColorStr='#66b84a', EndColorStr='#51933a');
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorStr='#66b84a', EndColorStr='#51933a')";
	
	background-color: rgb(81, 147, 58);
	border-color: rgb(34, 34, 34);
	vertical-align: middle;
	padding:0;
	display: inline-block;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px 5px 5px 5px;
	font-size:20px;
	width:100%;
	line-height: 54px;
	text-transform: uppercase;
	
}
a.convert:hover, button.convert:hover{
	color: #FFFFFF;
	cursor: pointer;
	-webkit-box-shadow: 0 1px 0 rgba(255,255,255,0.2);
	-moz-box-shadow: inset 0 1px 0 rgba(255,255,255,0.5), 0 1px 0 rgba(255,255,255,0.2), inset 0 1px 7px rgba(255,255,255,0.6);
	box-shadow: inset 0 1px 0 rgba(255,255,255,0.5), 0 1px 0 rgba(255,255,255,0.2), inset 0 1px 7px rgba(255,255,255,0.6);

}
a.convert:active, button.convert:active {
	-webkit-box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	-moz-box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.5);
	color: white;
	background: #777;
	
}

h1.gift-blurb-title {
	font-family: "proxima-nova",Arial;
	font-size: 30px;
	color: black;
	letter-spacing: -1px;
	margin-bottom: 12px;
