	color: #FFFFFF;
	cursor: pointer;
	-webkit-box-shadow: 0 1px 0 rgba(255,255,255,0.2);
	-moz-box-shadow: inset 0 1px 0 rgba(255,255,255,0.5), 0 1px 0 rgba(255,255,255,0.2), inset 0 1px 7px rgba(255,255,255,0.6);
	box-shadow: inset 0 1px 0 rgba(255,255,255,0.5), 0 1px 0 rgba(255,255,255,0.2), inset 0 1px 7px rgba(255,255,255,0.6);

}
a.convert:active, button.convert:active, .recurly button.submit:active {
	-webkit-box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	-moz-box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.5);
	color: white;
	background: #777;
	
}

a.secondary, input.convert.secondary {
	background-image: -moz-linear-gradient(center top , #ddd, #aaa);
	background-image:-webkit-gradient(linear,left top,left bottom,color-stop(0, #ddd),color-stop(1, #aaa));
	filter:progid:DXImageTransform.Microsoft.gradient(startColorStr='#ddd', EndColorStr='#aaa');
	-ms-filter:"progid:DXImageTransform.Microsoft.gradient(startColorStr='#ddd', EndColorStr='#aaa')";

	background-color:#ddd;
	color: #243342;
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
    
    border-radius: 3px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
    
    width: 150px;
    font-size: 13px;
	border:1px solid #ccc;

}

.style-chooser a.secondary, .style-chooser input.convert.secondary {
	width:87px;
	font-size: 10px;
}

.style-chooser a.secondary.t-shirts,.style-chooser a.secondary.shirts {
	width:131px;
}

a.convert.secondary:hover {
	color:black;
	border: 1px solid #ddd;
}

a.convert.secondary.active:hover {
	color:white;
}

a.convert.lt {
	display:block;
	float:left;
	border-radius: 3px 0 0 3px;
	-webkit-border-radius: 3px 0 0 3px;
	-moz-border-radius: 3px 0 0 3px;
	border: 1px solid #ccc;
    border-right: 1px solid #ddd;
    
}

a.convert.rt {
	display:block;
	float:left;
	border-radius: 0px 3px 3px 0px;
	-webkit-border-radius: 0px 3px 3px 0px;
	-moz-border-radius: 0px 3px 3px 0px;
	border: 1px solid #ccc;
    border-left: 1px solid #ddd;
    }

a.convert.mid {
	display:block;
	float:left;
	border-radius: 0;
	-webkit-border-radius:  0;
	-moz-border-radius:  0;
	border: 1px solid #ccc;
    border-left: 1px solid #ddd;
	

}

a.convert.active {
	-webkit-box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	-moz-box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	box-shadow: inset 0 3px 9px rgb(0,0,0), 0 1px 0 rgba(255,255,255,0.2);
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.5);
	color: white;
	background: #777;
	border:1px solid #888;
}

.product-info dd {
	margin: 5px 0pt 0pt;
	padding: 0pt 0pt 12px;
}
.product-info dd .input-box {
	display: block;
	height: 45px;
}


/* copied from keep-calm to-do create SASS mixin */

ul.plans-list a.convert, button.convert {
	width:100%;
	height:42px;
}

/* frequency chooser */
.category-plans .breadcrumbs {
	display:none;
}

.frequency-chooser-container {
	background:black;
	width:100%;	
}
.frequency-chooser-section {
	width:960px;
	margin:0 auto;
}

.frequency-chooser {
	padding:20px;
}

.frequency-chooser-text {
	float:left;
	font-family:"proxima-nova",Helvetica,Arial;
	font-weight:bold;
	font-size:13px;
	color:#eee;
	font-size: 15px;
    line-height: 43px;
    margin-right: 20px;
}

.lte9 .frequency-chooser a.convert.secondary {
	filter:none;
}

/* end frequency chooser */

#oa_social_login_precheckout {
	margin-top:40px;
}
.add-to-box {
	text-align: center;

}
.add-to-box span.or-text {
	font-family: "proxima-nova",Arial,sans-serif;
	font-weight:900;
	font-size: 20px;
	color: #595959;
}

.add-to-box a.convert {
	clear: both;
	width: 100%;
	margin: 25px 0;
}

#checkoutSteps button.convert {
	width: 30%;
	float: right;
	font-weight:normal;
	font-family: ff-enzo-web,"proxima-nova",Arial,sans-serif;
}

.custom-plan {
	margin-bottom: 25px;
}

/* recurly add-on popup tooltip */
.add_ons.any #popover {
	margin-left: 140px;
    margin-top: -170px;
}


